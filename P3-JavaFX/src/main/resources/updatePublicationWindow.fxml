<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="Update publication">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin></Label>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Title:">
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin></Label>
                        <TextField fx:id="titleTextField">
                     <HBox.margin>
                        <Insets left="95.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Authors:">
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin></Label>
                        <TextField fx:id="authorsTextField">
                     <HBox.margin>
                        <Insets left="72.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Publication year:">
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin></Label>
                        <TextField fx:id="yearTextField">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Publisher:">
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin></Label>
                        <TextField fx:id="publisherTextField">
                     <HBox.margin>
                        <Insets left="65.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <Button mnemonicParsing="false" onAction="#sendUpdatedPublication" text="Send">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Button>
            </children>
        </VBox>
    </children>
</StackPane>
