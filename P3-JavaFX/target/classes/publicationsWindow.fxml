<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button mnemonicParsing="false" onAction="#logOut" text="Log out">
               <VBox.margin>
                  <Insets bottom="10.0" left="400.0" />
               </VBox.margin></Button>
            <VBox fx:id="VBoxPublications" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Your publications">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <VBox fx:id="publicationsListVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" />
                  <Button mnemonicParsing="false" onAction="#clickAddPublication" text="Add publication">
                     <VBox.margin>
                        <Insets top="30.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
